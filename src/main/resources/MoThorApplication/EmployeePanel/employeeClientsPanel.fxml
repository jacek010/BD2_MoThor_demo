<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MoThorApplication.EmployeePanel.EmployeeClientsController">

    <children>
        <TableView fx:id="clientsListModelTableView" layoutX="10.0" layoutY="94.0" prefHeight="303.0" prefWidth="731.0">
            <columns>
                <TableColumn fx:id="clientIDTableColumn" prefWidth="53.0" text="Client ID" />
                <TableColumn fx:id="clientFirstNameTableColumn" prefWidth="99.0" text="First Name" />
                <TableColumn fx:id="clientLastNameTableColumn" prefWidth="94.0" text="Last Name" />
                <TableColumn fx:id="clientDrivingLicenseTableColumn" prefWidth="93.0" text="Driving License" />
                <TableColumn fx:id="clientPhoneNumberTableColumn" prefWidth="122.0" text="Phone Number" />
                <TableColumn fx:id="clientEmailAddressTableColumn" prefWidth="99.0" text="Email Address" />
                <TableColumn fx:id="clientPreviousOrdersTableColumn" prefWidth="101.0" text="Previous Orders" />
                <TableColumn fx:id="clientViewOrdersTableColumn" prefWidth="69.0" text="Orders" />
            </columns>
        </TableView>
        <Button fx:id="exitButton" layoutX="672.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="19.0" prefWidth="64.0" style="-fx-background-color: #1aa3ff;" text="Exit" textFill="WHITE">
            <font>
                <Font name="Segoe UI Black" size="12.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="30.0" layoutY="18.0" prefHeight="17.0" prefWidth="47.0" text="Search">
            <font>
                <Font name="Segoe UI Black" size="12.0" />
            </font>
        </Label>
        <TextField fx:id="keywordsClientsTextField" layoutX="84.0" layoutY="14.0" prefHeight="25.0" prefWidth="226.0" promptText="Keywords..." />
      <Button fx:id="logoutButton" layoutX="672.0" layoutY="46.0" mnemonicParsing="false" onAction="#logoutButtonOnAction" prefHeight="19.0" prefWidth="64.0" style="-fx-background-color: #1aa3ff;" text="Logout" textFill="WHITE">
         <font>
            <Font name="Segoe UI Black" size="12.0" />
         </font>
      </Button>
        <Label fx:id="loggedAsLabel" layoutX="327.0" layoutY="18.0" prefHeight="17.0" prefWidth="322.0" text="You are logged as: ">
         <font>
            <Font name="Segoe UI Black" size="12.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
