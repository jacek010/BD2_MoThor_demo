<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="MoThorApplication.Controllers.EmployeeEmployeesController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="carListModelTableView11" layoutX="10.0" layoutY="94.0" prefHeight="303.0" prefWidth="731.0">
            <columns>
                <TableColumn fx:id="employeeIDTableColumn" text="Employee ID" />
                <TableColumn fx:id="employeeFirstNameTableColumn" prefWidth="99.0" text="First Name" />
                <TableColumn fx:id="employeeLastNameTableColumn" prefWidth="132.0" text="Last Name" />
                <TableColumn fx:id="employeePhoneNumberTableColumn" prefWidth="158.0" text="Phone Number" />
                <TableColumn fx:id="employeeEmailAddressTableColumn" prefWidth="171.0" text="Email Address" />
                <TableColumn fx:id="promoteTableColumn" prefWidth="90.0" text="Promote" />
            </columns>
        </TableView>
        <Button fx:id="exitButton11" layoutX="672.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="19.0" prefWidth="64.0" style="-fx-background-color: #1aa3ff;" text="Exit" textFill="WHITE">
            <font>
                <Font name="Segoe UI Black" size="12.0" />
            </font>
        </Button>
        <Label alignment="CENTER" layoutX="30.0" layoutY="18.0" prefHeight="17.0" prefWidth="47.0" text="Search">
            <font>
                <Font name="Segoe UI Black" size="12.0" />
            </font>
        </Label>
        <TextField fx:id="keywordsEmployeesTextField" layoutX="84.0" layoutY="14.0" prefHeight="25.0" prefWidth="226.0" promptText="Keywords..." />
    </children>
</AnchorPane>
